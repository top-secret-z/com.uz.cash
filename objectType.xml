            <definitionname>com.uz.cash.condition.user</definitionname>
            <classname>wcf\system\condition\UserIntegerPropertyCondition</classname>
            <conditiongroup>contents</conditiongroup>
            <propertyname>activityPoints</propertyname>
            <minvalue>0</minvalue>
        </type>
        <type>
            <name>com.uz.cash.condition.likesReceived</name>
            <definitionname>com.uz.cash.condition.user</definitionname>
            <classname>wcf\system\condition\UserIntegerPropertyCondition</classname>
            <conditiongroup>contents</conditiongroup>
            <propertyname>likesReceived</propertyname>
            <minvalue>0</minvalue>
        </type>
        <type>
            <name>com.uz.cash.condition.trophyPoints</name>
            <definitionname>com.uz.cash.condition.user</definitionname>
            <classname>wcf\system\condition\UserIntegerPropertyCondition</classname>
            <conditiongroup>contents</conditiongroup>
            <propertyname>trophyPoints</propertyname>
            <minvalue>0</minvalue>
        </type>

        <type>
            <name>com.uz.cash.condition.credit.username</name>
            <definitionname>com.uz.cash.condition.credit.user</definitionname>
            <classname>wcf\system\condition\UserUsernameCondition</classname>
            <conditiongroup>general</conditiongroup>
        </type>
        <type>
            <name>com.uz.cash.condition.credit.email</name>
            <definitionname>com.uz.cash.condition.credit.user</definitionname>
            <classname>wcf\system\condition\UserEmailCondition</classname>
            <conditiongroup>general</conditiongroup>
        </type>
        <type>
