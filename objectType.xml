<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/2019/objectType.xsd">
    <import>
        <!-- Attachments -->
        <type>
            <name>com.uz.cash.claim</name>
            <definitionname>com.woltlab.wcf.attachment.objectType</definitionname>
            <classname>cash\system\attachment\ClaimAttachmentObjectType</classname>
        </type>
        <type>
            <name>com.uz.cash.credit</name>
            <definitionname>com.woltlab.wcf.attachment.objectType</definitionname>
            <classname>cash\system\attachment\CreditAttachmentObjectType</classname>
        </type>
        <type>
            <name>com.uz.cash.posting</name>
            <definitionname>com.woltlab.wcf.attachment.objectType</definitionname>
            <classname>cash\system\attachment\PostingAttachmentObjectType</classname>
        </type>

        <!-- box controllers -->
        <type>
            <name>com.uz.cash.Balance</name>
            <definitionname>com.woltlab.wcf.boxController</definitionname>
            <classname>cash\system\box\CashBalanceBoxController</classname>
        </type>
        <type>
            <name>com.uz.cash.Claims</name>
            <definitionname>com.woltlab.wcf.boxController</definitionname>
            <classname>cash\system\box\CashClaimsBoxController</classname>
        </type>

        <!-- Category -->
        <type>
            <name>com.uz.cash.category</name>
            <definitionname>com.woltlab.wcf.category</definitionname>
            <classname>cash\system\category\CashCategoryType</classname>
            <defaultpermission>1</defaultpermission>
        </type>

        <!-- message -->
        <type>
            <name>com.uz.cash.claim</name>
            <definitionname>com.woltlab.wcf.message</definitionname>
        </type>
        <type>
            <name>com.uz.cash.credit</name>
            <definitionname>com.woltlab.wcf.message</definitionname>
        </type>
        <type>
            <name>com.uz.cash.posting</name>
            <definitionname>com.woltlab.wcf.message</definitionname>
        </type>

        <!-- notification -->
        <type>
            <name>com.uz.cash.claim.notification</name>
            <definitionname>com.woltlab.wcf.notification.objectType</definitionname>
            <classname>cash\system\user\notification\object\type\ClaimUserNotificationObjectType</classname>
            <category>com.uz.cash</category>
        </type>
        <type>
            <name>com.uz.cash.credit.notification</name>
            <definitionname>com.woltlab.wcf.notification.objectType</definitionname>
            <classname>cash\system\user\notification\object\type\CreditUserNotificationObjectType</classname>
            <category>com.uz.cash</category>
        </type>

        <!-- payment type -->
        <type>
            <name>com.uz.cash.payment.type.claim</name>
            <definitionname>com.woltlab.wcf.payment.type</definitionname>
            <classname>cash\system\payment\type\ClaimPaymentType</classname>
        </type>

        <!-- rebuild data workers -->
        <type>
            <name>com.uz.cash.all</name>
            <definitionname>com.woltlab.wcf.rebuildData</definitionname>
            <classname>cash\system\worker\CashRebuildDataWorker</classname>
        </type>

        <!-- user conditions -->
        <type>
            <name>com.uz.cash.condition.username</name>
            <definitionname>com.uz.cash.condition.user</definitionname>
            <classname>wcf\system\condition\UserUsernameCondition</classname>
            <conditiongroup>general</conditiongroup>
        </type>
        <type>
            <name>com.uz.cash.condition.email</name>
            <definitionname>com.uz.cash.condition.user</definitionname>
            <classname>wcf\system\condition\UserEmailCondition</classname>
            <conditiongroup>general</conditiongroup>
